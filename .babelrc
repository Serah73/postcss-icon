{
	"env": {
		"modern": {
			"presets": [
				[
					"env",
					{
						"debug": true,
						"targets": {
							"node": ["7.6.0"]
						}
					}
				]
			]
		},
		"old": {
			"presets": [
				[
					"env",
					{
						"debug": true,
						"targets": {
							"node": ["4.2.0"]
						}
					}
				]
			]
		}
	},
	"plugins": [
		"transform-object-rest-spread",
		"add-module-exports",
		[
			"transform-inline-environment-variables",
			{
				"include": ["BABEL_ENV"]
			}
		]
	]
}
