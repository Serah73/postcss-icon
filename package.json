{
	"name": "postcss-icon",
	"description": "PostCSS plugin which allows you to quickly add styles to css icons",
	"version": "7.1.0",
	"repository": "https://github.com/retyui/postcss-icon",
	"author": "retyui <vamnemne@gmail.com>",
	"license": "MIT",
	"main": "postcss-icon.js",
	"files": [
		"lib"
	],
	"scripts": {
		"babel": "babel src --out-dir lib",
		"clean": "rimraf ./test/test.js ./lib ./lib-old",
		"compile": "npm-run-all --parallel babel testcompile",
		"lint": "eslint ./**/*.js --fix --quiet",
		"precompile": "npm-run-all clean lint",
		"prepublishOnly": "npm-run-all compile",
		"pretest": "npm run compile",
		"test": "mocha ./test/test.js",
		"testcompile": "babel test/test.mjs --out-file test/test.js"
	},
	"dependencies": {
		"fonteditor-core": "^1.0.5",
		"mkdirp-promise": "^5.0.1",
		"regenerator-runtime": "^0.12.1",
		"ttf2woff2": "^2.0.0"
	},
	"peerDependencies": {
		"postcss": "^6.0.0 || ^7.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0",
		"@babel/core": "^7.0.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/register": "^7.0.0",
		"babel-eslint": "^9.0.0",
		"babel-plugin-add-module-exports": "^1.0.0",
		"chai": "4.1.2",
		"eslint": "^5.6.0",
		"mocha": "^5.2.0",
		"npm-run-all": "^4.1.3",
		"postcss": "^7.0.0",
		"postcss-icon.font-awesome-v4": "^0.0.2",
		"postcss-icon.material-design": "^0.0.3",
		"postcss-merge-rules": "^4.0.1",
		"rimraf": "^2.6.2"
	},
	"keywords": [
		"css",
		"css-icon",
		"postcss",
		"postcss-plugin"
	],
	"engines": {
		"node": ">= 6.14"
	}
}
